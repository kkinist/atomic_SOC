***,Ca SO-CI
memory,1000,M;

gprint,orbitals,civector;

symmetry,xyz
geometry={Ca};

basis=aug-cc-pwCV5Z-PP

{rhf;wf,sym=1,spin=0}

! Active space (2/2)
{multi
    closed,1,3
    occ,3,3
    wf,sym=1,spin=0;state,2;
    expec2,lxx,lyy,lzz;
}
table, energy, ll
title, Energies and <L**2> values

core,0,0

{ci;wf,sym=1,spin=0;state,2; save,5201.2}
